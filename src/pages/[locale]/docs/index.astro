---
import DocsLayout from '../../../layouts/DocsLayout.astro';
import Breadcrumb from '../../../components/Breadcrumb.astro';
import { useTranslations, type Locale } from '../../../i18n/utils';

export async function getStaticPaths() {
	return [
		{ params: { locale: 'no' } },
		{ params: { locale: 'en' } },
	];
}

const { locale } = Astro.params as { locale: Locale };
const t = useTranslations(locale);
---

<DocsLayout
	locale={locale}
	title={t.meta.docs.title}
	description={t.meta.docs.description}
	currentPath={Astro.url.pathname}
>
	<article class="text-base/7 text-gray-700">
		<Breadcrumb items={[
			{ label: t.nav.home, href: `/${locale}` },
			{ label: t.nav.docs }
		]} />
		<p class="text-base/7 font-semibold text-indigo-600">{t.docs.tagline}</p>
		<h1 class="mt-2 text-4xl font-semibold tracking-tight text-pretty text-gray-900 sm:text-5xl">
			{t.docs.title}
		</h1>
		<p class="mt-6 text-xl/8">
			{t.docs.intro}
		</p>

		<!-- Quick Links -->
		<div class="mt-10 border-l border-indigo-600 pl-9">
			<h3 class="text-xl font-semibold text-gray-900">{t.docs.quickLinks.title}</h3>
			<div class="mt-6 grid grid-cols-1 md:grid-cols-2 gap-3 text-sm">
				<a href="#overview" class="text-indigo-600 hover:text-indigo-700">{t.docs.quickLinks.overview}</a>
				<a href="#sync" class="text-indigo-600 hover:text-indigo-700">{t.docs.quickLinks.sync}</a>
				<a href="#order-sync" class="text-indigo-600 hover:text-indigo-700">{t.docs.quickLinks.orderSync}</a>
				<a href="#job-system" class="text-indigo-600 hover:text-indigo-700">{t.docs.quickLinks.jobSystem}</a>
				<a href="#troubleshooting" class="text-indigo-600 hover:text-indigo-700">{t.docs.quickLinks.troubleshooting}</a>
				<a href="#support" class="text-indigo-600 hover:text-indigo-700">{t.docs.quickLinks.support}</a>
			</div>
		</div>

		<div class="mt-10 max-w-2xl">
			<h2 id="overview" class="mt-16 text-3xl font-semibold tracking-tight text-pretty text-gray-900">
				{t.docs.overview.title}
			</h2>
			<p class="mt-6">
				{t.docs.overview.intro}
			</p>
			<ul class="mt-8 space-y-3 text-gray-600">
				{t.docs.overview.features.map((feature) => (
					<li class="flex gap-x-3">
						<span class="text-indigo-600">✓</span>
						<span>{feature}</span>
					</li>
				))}
			</ul>

			<h3 class="mt-10 text-xl font-semibold text-gray-900">{t.docs.overview.requirementsTitle}</h3>
			<p class="mt-4">{t.docs.overview.requirementsIntro}</p>
			<ul class="mt-6 space-y-3 text-gray-600">
				{t.docs.overview.requirements.map((req) => (
					<li class="flex gap-x-3">
						<span class="text-indigo-600">•</span>
						<span><strong class="font-semibold text-gray-900">{req.label}</strong> {req.description}</span>
					</li>
				))}
			</ul>

			<figure class="mt-10 border-l border-indigo-600 pl-9">
				<blockquote class="font-semibold text-gray-900">
					<p>
						"{t.docs.overview.performanceNote}"
					</p>
				</blockquote>
			</figure>
		</div>

		<div class="mt-10 max-w-2xl">
			<h2 id="sync" class="mt-16 text-3xl font-semibold tracking-tight text-pretty text-gray-900">
				{t.docs.sync.title}
			</h2>

			<h3 class="mt-10 text-xl font-semibold text-gray-900">{t.docs.sync.flowTitle}</h3>
			<p class="mt-4">{t.docs.sync.flowIntro}</p>

			<ol class="mt-8 space-y-6 text-gray-600 list-decimal list-inside">
				{t.docs.sync.flowSteps.map((step) => (
					<li><strong class="font-semibold text-gray-900">{step.label}</strong> {step.description}</li>
				))}
			</ol>
		</div>

		<div class="mt-10 max-w-2xl">
			<h3 class="mt-10 text-xl font-semibold text-gray-900">{t.docs.sync.whatSyncTitle}</h3>

			<p class="mt-6 font-semibold text-gray-900">{t.docs.sync.productInfo.title}</p>
			<ul class="mt-4 space-y-2 text-gray-600 text-sm">
